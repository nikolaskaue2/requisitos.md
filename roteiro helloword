#include <stdio.h>
#include <locale.h>

int main() {
    printf("Hello Word!");
    return 0;
}

int main() {
    setlocale(LC_ALL, "Portuguese");
    
    printf("Ola, Mundo!\n");
    
    return 0;
}

int main() {
    float Valor2, Valor3, Total;
    
    Valor1 = 10.5;
    Valor2 = 20.5;
    Valor3 = 5.75;
    
    Total = Valor1 + Valor2 + Valor3;
    printf("Total:%.2f", Total);
    
    return 0;
}

int main() {
    int idade;
    
    printf("digite sua idade: ");
    scanf("%d", &idade);
    
    printf("VocÃª tem %d anos.\n" idade);
    return 0;
}
